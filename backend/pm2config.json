{
  "apps": [{
    "name"	: "server",
    "script": "src/server.js",
    "watch":  ["views", "public", "routes"],
    "env": {
      "NODE_ENV": "development"
    },
    "env_production": {
      "NODE_ENV": "production"
    },
    "log_date_format":  "MM-DD-YYYY HH:mm Z",
    "error_file":  "myapp_err.log",
    "out_file":  "myapp_out.log"
  }]
}